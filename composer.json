{
    "name": "juanchosl/assetminifyer",
    "description": "Simple library in order to minify js and css files used as assets into web projects",
    "type": "library",
    "license": "MIT",
    "homepage": "https://github.com/JuanchoSL/AssetMinifyer",
    "minimum-stability": "stable",
    "keywords": [
        "minifyer",
        "css",
        "js"
    ],
    "authors": [
        {
            "name": "Juan SÃ¡nchez Lecegui",
            "email": "JuanchoSL@hotmail.com",
            "homepage": "https://github.com/JuanchoSL/"
        }
    ],
    "require": {
        "php": "^8.1",
        "tedivm/jshrink": "~1.0",
        "juanchosl/exceptions": "1.0.*"
    },
    "require-dev": {
        "phpunit/phpunit": "9.6.*",
        "phpstan/phpstan": "1.10.*"
    },
    "autoload": {
        "psr-4": {
            "JuanchoSL\\AssetMinifyer\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "JuanchoSL\\AssetMinifyer\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "tests": "vendor/bin/phpunit --configuration phpunit.xml tests",
        "phpstan": "vendor/bin/phpstan analyse -c phpstan.neon"
    }
}